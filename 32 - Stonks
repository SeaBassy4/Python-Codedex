stock_prices = [34.68, 36.09, 34.94, 33.97, 34.68, 35.82, 43.41, 44.29, 44.65, 53.56, 49.85, 48.71, 48.71, 49.94, 48.53, 47.03, 46.59, 48.62, 44.21, 47.21]

def price_at(i): 
  return stock_prices[i-1]

def max_price(a,b):
  start = min(a, b)
  end = max(a, b)
  return max(stock_prices[start-1:end])

def min_price(a,b):
  start = min(a,b)
  end = max(a,b)
  return min(stock_prices[start-1:end])


print(price_at(1))
print(min_price(1,5))
print(max_price(1,5))