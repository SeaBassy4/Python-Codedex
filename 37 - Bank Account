class BankAccount: 
    def __init__(self, first_name, last_name, account_id, account_type, pin, balance):
        self.first_name = first_name
        self.last_name = last_name
        self.account_id = account_id
        self.account_type = account_type
        self.pin = pin
        self.balance = balance

    def deposit(self):
        add_money = int(input("Add to balance: "))
        self.balance = self.balance + add_money
        return self.balance
  
    def withdraw(self):
        sub_money = int(input("Taking out: "))
        self.balance = self.balance - sub_money
        return self.balance

    def display_balance(self):
        print(self.balance)


Seabassy = BankAccount("Sebastian", "Perez", 1234, "Debit", 1234, 96.0)

Seabassy.deposit()
Seabassy.withdraw()
Seabassy.display_balance()